{
  "name": "yoast/helpscout-docs-api",
  "description": "Sync content to the Helpscout docs API",
  "keywords": [
    "yoast",
    "helpscout",
    "docs"
  ],
  "homepage": "https://yoast.com/wordpress/plugins/helpscout-docs-api/",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Joost de Valk",
      "email": "support@yoast.com",
      "homepage": "https://yoast.com"
    }
  ],
  "type": "wordpress-plugin",
  "require": {
    "php": "^5.6.20||^7.0",
    "composer/installers": "^1.9.0"
  },
  "require-dev": {
    "php-parallel-lint/php-console-highlighter": "^0.5",
    "php-parallel-lint/php-parallel-lint": "^1.2.0",
    "symfony/config": "^3.4",
    "yoast/yoastcs": "^2.1.0",
    "yoast/php-development-environment": "^1.0",
    "yoast/wp-test-utils": "^0.2.1",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "classmap": [
      "admin/",
      "includes/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "config/"
    ]
  },
  "scripts": {
    "fix-cs": [
      "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf"
    ],
    "cs": [
      "Yoast\\HelpScout_Docs_API\\Config\\Composer\\Actions::check_coding_standards"
    ],
    "check-staged-cs": [
      "@check-cs-warnings --filter=GitStaged"
    ],
    "check-branch-cs": [
      "Yoast\\HelpScout_Docs_API\\Config\\Composer\\\\Actions::check_branch_cs"
    ],
    "check-cs": [
      "@check-cs-warnings -n"
    ],
    "check-cs-errors": [
      "echo You can now just use check-cs, running that command now.",
      "composer check-cs"
    ],
    "check-cs-warnings": [
      "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs"
    ]
  }
}
